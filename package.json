    "ngx-indexed-db": "19.0.0",
    "ngx-skeleton-loader": "9.0.0",
    "ngx-whats-new": "0.4.0",
    "node-mpv": "github:4gray/Node-MPV",
    "rxjs": "7.8.1",
    "semver": "7.5.2",
    "uuid": "9.0.0",
    "video.js": "7.20.3",
    "videojs-contrib-quality-levels": "2.2.0",
    "videojs-hls-quality-selector": "1.1.4",
    "zone.js": "~0.14.10"
  },
