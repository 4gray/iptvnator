        "@eslint/eslintrc": "3.1.0",
        "@eslint/js": "9.9.1",
        "@ngrx/eslint-plugin": "^18.0.2",
        "@ngx-translate/core": "15.0.0",
        "@ngx-translate/http-loader": "8.0.0",
        "@playwright/test": "1.46.1",
        "@semantic-release/changelog": "6.0.3",
