        "@typescript-eslint/eslint-plugin-tslint": "7.0.2",
        "@typescript-eslint/parser": "^8.2.0",
        "conventional-changelog-cli": "5.0.0",
        "electron": "27.1.0",
        "electron-builder": "24.6.4",
        "eslint": "^9.9.1",
        "eslint-plugin-import": "2.29.1",
        "globals": "15.9.0",
