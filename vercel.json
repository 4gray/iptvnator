{
  "version": 2,
  "name": "ruvo-player-api",
  "buildCommand": "cd api && npm install",
  "outputDirectory": ".",
  "installCommand": "cd api && npm install",
  "framework": null,
  "functions": {
    "api/parse.js": {
      "maxDuration": 30
    },
    "api/xtream.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/parse",
      "dest": "/api/parse.js"
    },
    {
      "src": "/api/xtream",
      "dest": "/api/xtream.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}