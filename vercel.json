{
  "version": 2,
  "functions": {
    "api/parse.js": {
      "maxDuration": 30
    },
    "api/xtream.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/parse",
      "dest": "/api/parse.js"
    },
    {
      "src": "/api/xtream",
      "dest": "/api/xtream.js"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}